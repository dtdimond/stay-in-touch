<% if obj.errors.any? %>
  <div class="container-narrow">
  <div class="alert alert-danger" role="alert">
    <p><h4>Oops, there were some problems.</h4></p>
    <ul>
      <% obj.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
  </div>
<% end %>
